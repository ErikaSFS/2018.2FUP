#include <stdio.h>

int main(){
    int n=0, ant = 1, atual= 1, prox=0, i =2;
    scanf("%d", &n);
    
    if(n==1 || n==2){
        printf("1\n");
        return 0;
        
    } else {
        while( i < n){
            prox= ant+atual;
            ant = atual;
            atual = prox;
            
            i++;
        
}
printf("%d", prox);


    }
}
